{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "create-env": "node ./bin/create-env",
    "check-env": "node -e \"require('dotenv').config(); console.log('Environment loaded successfully!')\"",
    "setup": "npm run create-env && npm install",
    "setup-db": "node ./bin/setup-db",
    "db-health": "node -e \"require('dotenv').config(); const {db} = require('./lib/database'); db.healthCheck().then(h => console.log(JSON.stringify(h, null, 2))).then(() => db.closePool())\""
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^17.2.4",
    "express": "~4.16.1",
    "jsonwebtoken": "^9.0.3",
    "morgan": "~1.9.1",
    "mysql2": "^3.16.3"
  }
}
